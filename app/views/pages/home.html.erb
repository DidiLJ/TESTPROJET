<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>


<%= simple_form_for(@article) do |f| %>
  <%= f.input :photo, as: :file %>
  <%= cl_image_tag @article.photo.key, height: 300, width: 400, crop: :fill %>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.submit %>

<% end %>
